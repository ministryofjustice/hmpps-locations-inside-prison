{% extends "../../partials/layout.njk" %}

{% from "govuk/components/table/macro.njk" import govukTable %}

{% block content %}
  <div class="govuk-width-container">
    <div class="govuk-grid-row">
      <div class="govuk-grid-column-{{ minLayout }}">
        {{ govukTable({
          head: [
            { text: "Type of change" },
            { text: "Changed from" },
            { text: "Changed to" },
            { text: "Changed by" },
            { text: "Date" }
          ],
          rows: tableRows
        }) }}
      </div>
    </div>
  </div>
{% endblock %}
